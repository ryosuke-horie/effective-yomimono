		"@types/node": "^22.13.14",
		"@types/react": "^19",
		"@types/react-dom": "19.0.4",
		"@vitejs/plugin-react": "^4.3.4",
		"@vitest/coverage-v8": "^3.1.1",
		"jsdom": "^26.0.0",
